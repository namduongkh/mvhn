{
  "name": "mucngay",
  "version": "1.0.0",
  "description": "Project for C2C bussiness",
  "main": "index.js",
  "scripts": {
    "clean:dev": "rimraf public/src && rimraf public/cms/src",
    "clean:prod": "rimraf public/dist && rimraf public/cms/dist",
    "start": "npm-run-all --parallel clean:dev develop develop_cms nodemon",
    "nodemon": "nodemon --inspect -r esm app.js",
    "develop": "webpack --env.env=dev --env.dir=web --watch --progress --profile --colors",
    "develop_cms": "webpack --env.env=dev --env.dir=cms --watch --progress --profile --colors",
    "build": "yarn run clean:prod && webpack --env.env=prod --env.dir=web --progress --profile --colors && webpack --env.env=prod --env.dir=cms --progress --profile --colors",
    "test": "mocha",
    "test:nw": "nightwatch",
    "test:live": "karma start --auto-watch --no-single-run",
    "checkcode": "./node_modules/.bin/eslint ./app -f html  > code-report/code-format.html; open code-report",
    "fixcode": "./node_modules/.bin/eslint ./app --fix -f html  > code-report/code-format.html; open code-report",
    "check-copy": "checkcode -p './app/templates/web/html' -f '**/*.html' -l html code-report/html.html;checkcode -p './app/templates/web/html' -f '**/*.scss' -l css code-report/css.html;checkcode -p './app/templates/web/html' -f '**/*.js' -l js code-report/js.html;open code-report"
  },
  "author": "Phong Nguyá»…n",
  "license": "ISC",
