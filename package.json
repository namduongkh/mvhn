    "jimp": "^0.2.27",
    "kea-config": "^0.2.3",
    "lodash": "^4.17.4",
    "moment": "^2.24.0",
    "mongoose": "^4.9.6",
    "mongoose-pagination": "^1.0.0",
    "nodemailer": "^4.7.0",
